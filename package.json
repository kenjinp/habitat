{
  "name": "habitat",
  "author": "Kenneth Pirman <github@kenny.wtf>",
  "description": "Vignette of an O'Neill cylinder habitat",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "repository": "git@github.com:kenjinp/habitat",
  "scripts": {
    "start": "pnpm dev",
    "dev": "turbo run dev --filter=examples",
    "build": "turbo run build --filter=examples",
    "build:packages": "turbo run build --filter=./packages/*",
    "preinstall": "npx only-allow pnpm",
    "test": "jest --passWithNoTests",
    "ship": "pulumi up -C=./infra/pulumi -s=dev --yes --skip-preview"
  },
  "dependencies": {
    "@hello-worlds/core": "^0.0.9",
    "@hello-worlds/planets": "0.0.24",
    "@hello-worlds/react": "0.0.27",
    "@react-three/drei": "9.88.7",
    "@react-three/fiber": "8.15.8",
    "@react-three/postprocessing": "^2.15.11",
    "camera-controls": "^2.7.3",
    "leva": "^0.9.27",
    "postprocessing": "^6.33.4",
    "r3f-perf": "^6.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "0.158.0",
    "three-custom-shader-material": "^4.0.0",
    "three-stdlib": "^2.23.10"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.2",
    "@types/node": "18.11.10",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.0",
    "@types/three": "^0.141.0",
    "prettier": "^2.8.1",
    "prettier-plugin-glsl": "^0.0.5",
    "prettier-plugin-organize-imports": "^3.2.3"
  },
  "workspaces": [
    "infra/*",
    "packages/*",
    "apps/*"
  ]
}
